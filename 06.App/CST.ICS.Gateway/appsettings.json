{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug", //最小日志记录级别
      "Override": { //系统日志最小记录级别
        "Default": "Debug",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {SourceContext} {ThreadId} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Debug"
      },
      {
        "Name": "Async", //异步写入日志
        "Args": {
          "configure": [
            {
              "Name": "File", //输出文件
              "Args": {
                "path": "log/Warning/log.txt",
                "RestrictedToMinimumLevel": "Warning",
                "outputTemplate": "{NewLine}Date:{Timestamp:yyyy-MM-dd HH:mm:ss.fff}{NewLine}LogLevel:{Level}{NewLine}Class:{SourceContext}{NewLine}Message:{Message}{NewLine}{Exception}",
                "rollingInterval": "3", //日志文件生成精度：1：年  2：月 3：日 4：小时
                "shared": true,
                "rollOnFileSizeLimit": true,
                "fileSizeLimitBytes": 102400000,
                "retainedFileCountLimit": 365
              }
            }
          ]
        }
      }
    ]
  },
  "TopicSet": {
    "UUTTopic": {
      "SubTopics": [
        {
          "TopicClass": "客户端与被检",
          "TopicValue": "/{0}/D"
        }
      ],
      "PubTopics": [
        {
          "TopicClass": "客户端与被检",
          "TopicValue": "/{0}/D/{1}"
        }
      ]
    },
    "ENVTopic": {
      "SubTopics": [
        {
          "TopicClass": "客户端与压源设备",
          "TopicValue": "/{0}/E/PressureControl"
        },
        {
          "TopicClass": "客户端与压表设备",
          "TopicValue": "/{0}/E/PressureSTD"
        },
        {
          "TopicClass": "客户端与温源设备",
          "TopicValue": "/{0}/E/TemperatureControl"
        },
        {
          "TopicClass": "客户端与温表设备",
          "TopicValue": "/{0}/E/TemperatureControl"
        }

      ],
      "PubTopics": [
        {
          "TopicClass": "客户端与压源设备",
          "TopicValue": "/{0}/E/PressureControl"
        },
        {
          "TopicClass": "客户端与压表设备",
          "TopicValue": "/{0}/E/PressureSTD"
        },
        {
          "TopicClass": "客户端与温源设备",
          "TopicValue": "/{0}/E/TemperatureControl"
        },
        {
          "TopicClass": "客户端与温表设备",
          "TopicValue": "/{0}/E/TemperatureControl"
        }
      ]
    }
  }


}
